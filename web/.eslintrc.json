{
    "env": {
        "browser": true,
        "es6": true,
        "jasmine": true
    },
    "parser": "babel-eslint",
    "extends": [
      "eslint:recommended",
      "google",
      "plugin:react/recommended"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": 0,
        "indent": [
            "error",
            2,
            {"SwitchCase": 1}
        ],
        "require-jsdoc": 0
    },
    "settings": {
        "react": {
            "version": "detect"
        }
    }
}
